<h1>Edit Paddock</h1>

<form action="/paddocks/$paddock.getId()" method="post">

    <label for="name">Name:</label>
    <input type="inputbox" id="name" name="name" value="$paddock.getName()">

    <label for="park">Park:</label>
    <select name="park" id="park" selected="$paddock.getPark.getName()">

        #foreach ($park in $parks)
        <option value="$park.getId()">$park.getName()</option>
        #end
    </select>

    <input type="submit" value="save">

</form>