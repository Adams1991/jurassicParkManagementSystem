<h1>Move Dino</h1>

<form action="/herbivores/$herbivore.getId()" method="post">

    <label for="name">Name:</label>
    <input type="inputbox" id="name" name="name" value="$herbivore.getName()">

    <label for="paddock">Paddock:</label>
    <select name="paddock" id="paddock">
        #foreach ($paddock in $paddocks)
        <option value="$paddock.getId()">$paddock.getName()</option>
        #end
    </select>


    <input type="submit" value="save">

</form>